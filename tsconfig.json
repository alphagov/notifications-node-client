{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationDir": "types",
    "strict": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "es2015",
    "module": "commonjs"
  },
  "include": ["client/notification.js", "index.js"],
  "exclude": ["node_modules", "spec"]
}
