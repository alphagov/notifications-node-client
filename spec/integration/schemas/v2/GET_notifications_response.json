{
    "$schema": "http://json-schema.org/schema#",
    "description": "GET list of notifications response schema",
    "type": "object",
    "properties": {
        "notifications": {
            "type": "array",
            "items": {
                "type": "object",
                "$ref": "notification.json"
            }
        },
        "links": {
            "type": "object",
            "properties": {
                "current": {
                    "type": "string"
                },
                "next": {
                    "type": "string"
                }
            },
            "additionalProperties": true,
            "required": ["current"]
        }
    },
    "additionalProperties": true,
    "required": ["notifications", "links"]
}
